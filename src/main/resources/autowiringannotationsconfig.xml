<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config/>

    <bean class="com.springframework.autowiring.annotations.Jaydeep" name="jaydeep">
        <property name="relation" value="Friends" />
    </bean>

    <bean class="com.springframework.autowiring.annotations.Jaydeep" name="jaydeep1">
        <property name="relation" value="Best Friends" />
    </bean>

    <bean class="com.springframework.autowiring.annotations.Bhavikk" name="bhavikk" />
</beans>